@{
}
@model IEnumerable<Order>


<h1 class="display-2 text-center">Paid Orders List</h1>

<table class="table">
    <thead>
    <th scope="col">Customer ID</th>
    <th scope="col">Order Total</th>
    <th scope="col">Order Paid At</th>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {

            <tr>
                <th scope="row">@order.UserId</th>>
                <td>@order.Total</td>
                <td>@order.FinishedAt</td>
            </tr>
        }
    </tbody>
</table>
